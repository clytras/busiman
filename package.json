    "styled-components": "^5.0.1",
    "tedious": "^8.0.1",
    "typeface-roboto": "^0.0.75",
    "use-global-hook": "^0.1.12",
    "validator": "^12.2.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "yarn": "^1.22.0"
  },
  "dependencies": {
    "sqlite3": "^5.0.0"
  }
}
