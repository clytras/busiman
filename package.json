    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "glob-fs": "^0.1.7",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "immutability-helper": "^3.0.1",
    "is-electron-renderer": "^2.0.1",
    "jest": "^25.1.0",
    "jest-extended": "^0.11.5",
    "knex": "^0.20.9",
    "luxon": "^1.22.0",
    "mdi-material-ui": "^6.11.0",
    "mkdir": "^0.0.2",
    "mssql": "^6.0.1",
    "mysql": "^2.18.1",
