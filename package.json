{
  "name": "businessman",
  "shortName": "BusinessMan",
  "productName": "Business Manager",
  "version": "0.0.1",
  "description": "A business organizer application",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "i": "yarn install --network-timeout 1000000000",
    "start3": "./node_modules/.bin/electron ./dist/main.bundle.js",
    "electron": "electron . --dev --dbg",
    "start4": "nodemon ./src/main.js --exec \"yarn run electron\"",
    "start": "nodemon --watch ./src/renderer/ && yarn run electron",
    "dev": "webpack --watch --config ./webpack.dev.js --progress --colors",
    "hot": "webpack-dev-server --hot --config ./webpack.hot.js",
    "dev-run": "yarn run dev && yarn run electron",
    "development": "rimraf dist && webpack --config ./webpack.dev.js && webpack --config ./webpack.hot.js",
    "dev:main": "webpack --config ./webpack.dev.js",
    "dev:electron": "yarn run dev:main && yarn run electron",
    "production": "rimraf dist && webpack --config ./webpack.prod.js --progress --colors",
    "bundle": "webpack --watch --config ./webpack.dev.js --progress --colors --mode development",
    "serve": "electron ./dist/main.bundle.js",
    "start2": "npm-run-all --parallel bundle serve",
    "postinstall": "electron-builder install-app-deps",
    "dist": "rimraf release && electron-builder",
    "clean:release": "rimraf release",
    "babel-run": "npx -p @babel/core -p @babel/node babel-node",
    "genTestDbConfigFile": "babel-run ./scripts/genTestDbConfigFile.js",
    "test:db:reset": "node ./scripts/resetTestDb.js",
    "test:db": "jest \"db\\.(.*)\\.test\\.js\" --verbose",
    "test:config": "jest \"config\\.(.*)\\.test\\.js\" --verbose",
    "test:utils": "jest \"utils\\.(.*)\\.test\\.js\" --verbose",
    "test": "jest --verbose"
  },
  "author": "Christos Lytras <christos.lytras@gmail.com>",
  "license": "MIT",
  "browserslist": "electron >= 7.1",
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended"
    ]
  },
  "build": {
    "productName": "BusinessMan",
    "copyright": "Copyright Â© 2020 ${author}",
    "appId": "gr.softx.BusinessMan",
    "files": [
      "dist/",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "publish": {
      "provider": "github",
      "owner": "clytras",
      "repo": "busiman",
      "private": false
    }
  },
  "resolutions": {
    "**/**/set-value": "^3.0.1",
    "**/**/braces": "^2.3.1",
    "**/**/lodash": "^4.17.12",
    "**/**/minimatch": "^3.0.2",
    "**/**/serialize-javascript": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/runtime": "^7.8.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@hot-loader/react-dom": "^17.0.0",
    "@lytrax/custom-electron-titlebar": "^3.1.8",
    "@material-ui/core": "^4.9.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.42",
    "add": "^2.0.6",
    "aphrodite": "^2.4.0",
    "app-root-path": "^3.0.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "bootstrap": "^4.4.1",
    "clsx": "^1.1.0",
    "color": "^3.1.2",
    "colors": "^1.4.0",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.4",
    "cp-cli": "^2.0.0",
    "cross-env": "^7.0.0",
    "csp-html-webpack-plugin": "^3.0.4",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "electron": "9.3.3",
    "electron-builder": "^22.2.0",
    "electron-hot-loader": "^0.8.0",
