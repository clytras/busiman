{
  "name": "businessman",
  "shortName": "BusinessMan",
  "productName": "Business Manager",
  "version": "0.0.1",
  "description": "A business organizer application",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start3": "./node_modules/.bin/electron ./dist/main.bundle.js",
    "electron": "electron . --dev --dbg",
    "start4": "nodemon ./src/main.js --exec \"yarn run electron\"",
    "start": "nodemon --watch ./src/renderer/ && yarn run electron",
    "dev": "webpack --watch --config ./webpack.dev.js --progress --colors",
    "hot": "webpack-dev-server --hot --config ./webpack.hot.js",
    "dev-run": "yarn run dev && yarn run electron",
    "development": "rimraf dist && webpack --config ./webpack.dev.js && webpack --config ./webpack.hot.js",
    "dev:main": "webpack --config ./webpack.dev.js",
    "dev:electron": "yarn run dev:main && yarn run electron",
    "production": "rimraf dist && webpack --config ./webpack.prod.js --progress --colors",
    "bundle": "webpack --watch --config ./webpack.dev.js --progress --colors --mode development",
    "serve": "electron ./dist/main.bundle.js",
    "start2": "npm-run-all --parallel bundle serve",
    "postinstall": "electron-builder install-app-deps",
    "dist": "rimraf release && electron-builder",
    "clean:release": "rimraf release",
    "genTestDbConfigFile": "npx -p @babel/core -p @babel/node babel-node ./scripts/genTestDbConfigFile.js",
    "test:db:reset": "node ./scripts/resetTestDb.js",
    "test:db": "jest \"db\\.(.*)\\.test\\.js\" --verbose",
    "test:config": "jest \"config\\.(.*)\\.test\\.js\" --verbose",
    "test:utils": "jest \"utils\\.(.*)\\.test\\.js\" --verbose"
  },
  "author": "Christos Lytras <christos.lytras@gmail.com>",
  "license": "MIT",
  "browserslist": "electron >= 7.1",
  "jest": {
    "setupFilesAfterEnv": ["jest-extended"]
  },
  "build": {
    "productName": "BusinessMan",
    "copyright": "Copyright Â© 2020 ${author}",
    "appId": "gr.softx.BusinessMan",
    "files": [
      "dist/",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "publish": {
      "provider": "github",
      "owner": "clytras",
      "repo": "busiman",
      "private": false
    }
  },
  "resolutions": {
    "**/**/set-value": "^3.0.1",
    "**/**/braces": "^2.3.1",
    "**/**/lodash": "^4.17.12",
    "**/**/minimatch": "^3.0.2",
    "**/**/serialize-javascript": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.7.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@hot-loader/react-dom": "^16.11.0",
    "@lytrax/custom-electron-titlebar": "^3.1.8",
    "@material-ui/core": "^4.7.1",
    "@material-ui/icons": "^4.5.1",
    "aphrodite": "^2.4.0",
    "app-root-path": "^3.0.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "bootstrap": "^4.4.1",
    "clsx": "^1.0.4",
    "colors": "^1.4.0",
    "copy-webpack-plugin": "^5.0.5",
    "core-js": "^3.4.7",
    "cp-cli": "^2.0.0",
    "cross-env": "^6.0.3",
    "csp-html-webpack-plugin": "^3.0.4",
    "css-loader": "^3.2.0",
    "dotenv": "^8.2.0",
    "electron": "7.1.2",
    "electron-builder": "^21.2.0",
    "electron-hot-loader": "^0.8.0",
    "electron-reload": "^1.5.0",
    "extract-loader": "^3.1.0",
    "file-loader": "^5.0.2",
    "framer-motion": "^1.7.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "glob-fs": "^0.1.7",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "is-electron-renderer": "^2.0.1",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.2",
    "knex": "^0.20.3",
    "luxon": "^1.21.3",
    "mdi-material-ui": "^6.9.0",
    "mkdir": "^0.0.2",
    "mysql2": "^2.0.1",
    "nanoid": "^2.1.8",
    "node-emoji": "^1.10.0",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "objection": "^2.0.3",
    "platform": "^1.3.5",
    "react": "^16.12.0",
    "react-accessible-accordion": "^2.4.5",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-localization": "^1.0.15",
    "react-new-window": "^0.1.2",
    "react-popper": "^1.3.6",
    "react-router-dom": "^5.1.2",
    "react-splitter-layout": "^4.0.0",
    "react-transition-group": "^4.3.0",
    "reactstrap": "^8.1.1",
    "read-data": "^1.1.0",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "styled-components": "^4.4.1",
    "tedious": "^6.6.2",
    "typeface-roboto": "^0.0.75",
    "validator": "^12.1.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "color": "^3.1.2",
    "sqlite3": "^4.1.0"
  }
}
